<?xml version="1.0" encoding="UTF-8"?>

<template xml:space="preserve">

        <t t-extend="FormView.buttons">
            <t t-jquery="button.o_form_button_edit" t-operation="after">
                <button t-if="widget.modelName == 'pos.session'" type="button"
